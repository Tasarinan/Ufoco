{
  "name": "Ufoco",
  "version": "1.0.0",
  "description": "Get your edge back",
  "license": "MIT",
  "private": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tasarinan/ufoco.git"
  },
  "author": {
    "name": "tasarinan",
    "email": "jben@sohu.com",
    "url": "https://github.com/tasarinan"
  },
  "bugs": {
    "url": "https://github.com/tasarinan/ufoco/issues"
  },
  "keywords": [
    "react",
    "electron",
    "webpack",
    "redux",
    "react-electron",
    "react-electron-webpack",
    "react-webpack",
    "react-babel",
    "webpack",
    "es6",
    "babel-7",
    "less",
    "sass"
  ],
  "engines": {
    "node": ">=8.0.0",
    "npm": ">=5.0.0",
    "yarn": ">=1.0.0"
  },
  "homepage": "https://github.com/tasarinan/ufoco#readme",
  "main": "bootstrapper.js",
  "scripts": {
    "start": "npm run env:dev -- webpack-dev-server",
    "build": "npm run env:prod -- webpack",
    "prod": "npm run build && electron --noDevServer .",
    "package": "npm run build",
    "package:all": "npm run build",
    "test": "jest -u",
    "lint": "eslint ./src",
    "docs": "jsdoc -r ./src -d docs --verbose",
    "env:dev": "cross-env NODE_ENV=development",
    "env:prod": "cross-env NODE_ENV=production",
    "build:clean": "rimraf -rf dist",
    "build:copy": "copyfiles -u 1 public/* public/**/* dist -e public/index.html",
    "prebuild": "npm run build:clean && npm run build:copy",
    "prepackage:all": "rimraf -rf release",
    "postpackage": "electron-builder build",
    "postpackage:all": "electron-builder build -mwl"
  },
  "dependencies": {
    "@draft-js-plugins/alignment": "^4.1.0",
    "@draft-js-plugins/anchor": "^4.1.1",
    "@draft-js-plugins/buttons": "^4.1.0",
    "@draft-js-plugins/drag-n-drop": "^4.1.0",
    "@draft-js-plugins/drag-n-drop-upload": "^4.1.0",
    "@draft-js-plugins/editor": "^4.1.0",
    "@draft-js-plugins/focus": "^4.1.0",
    "@draft-js-plugins/image": "^4.1.0",
    "@draft-js-plugins/inline-toolbar": "^4.1.0",
    "@draft-js-plugins/resizeable": "^4.1.0",
    "@draft-js-plugins/static-toolbar": "^4.1.0",
    "@draft-js-plugins/video": "^4.1.0",
    "classnames": "^2.2.6",
    "connected-react-router": "^6.8.0",
    "draft-js": "^0.11.7",
    "electron-log": "^3.0.9",
    "electron-unhandled": "^3.0.2",
    "electron-updater": "^4.3.5",
    "feather-icons": "^4.28.0",
    "flatpickr": "^4.6.6",
    "fs-extra": "^9.0.1",
    "history": "^4.10.1",
    "lodash-id": "^0.14.0",
    "lodash.debounce": "^4.0.8",
    "lowdb": "^1.0.0",
    "markdown-draft-js": "^2.2.1",
    "minireset.css": "0.0.6",
    "minisearch": "^2.6.2",
    "moment": "^2.29.1",
    "prop-types": "^15.7.2",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-feather": "^2.0.8",
    "react-hot-loader": "^4.13.0",
    "react-redux": "^7.2.1",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-router-redux": "^4.0.8",
    "redux": "^4.0.5",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "remixicon": "^2.5.0",
    "reselect": "^4.0.0",
    "semver": "^7.3.2",
    "shortid": "^2.2.15",
    "yup": "^0.29.3"
  },
  "devDependencies": {
    "@babel/core": "^7.12.0",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-transform-modules-commonjs": "^7.10.4",
    "@babel/plugin-transform-runtime": "^7.12.0",
    "@babel/preset-env": "^7.12.0",
    "@babel/preset-react": "^7.10.4",
    "@babel/register": "^7.12.0",
    "@hot-loader/react-dom": "^16.13.0",
    "antd-scss-theme-plugin": "^1.0.8",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-add-module-exports": "^1.0.4",
    "babel-plugin-import": "^1.13.1",
    "babel-plugin-root-import": "^6.5.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "babili-webpack-plugin": "^0.1.2",
    "brotli-webpack-plugin": "^1.1.0",
    "copyfiles": "^2.4.0",
    "cross-env": "^5.2.1",
    "css-hot-loader": "^1.4.4",
    "css-loader": "^2.1.1",
    "electron": "^15.5.5",
    "electron-builder": "^22.9.1",
    "electron-devtools-installer": "^2.2.4",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.5",
    "enzyme-to-json": "^3.6.1",
    "eslint": "^5.16.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-react": "^7.21.4",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.9.0",
    "jest-cli": "^24.9.0",
    "jsdoc": "^3.6.6",
    "less": "^3.12.2",
    "less-loader": "^4.1.0",
    "mini-css-extract-plugin": "^0.6.0",
    "node-sass": "^4.14.1",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-nested": "^4.2.3",
    "postcss-preset-env": "^6.6.0",
    "postcss-pxtorem": "^4.0.1",
    "rimraf": "^2.7.1",
    "sass-loader": "^7.3.1",
    "style-loader": "^0.23.1",
    "thread-loader": "^2.1.3",
    "uglifyjs-webpack-plugin": "^2.1.2",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^4.2.2"
  },
  "build": {
    "productName": "Sidekiff",
    "appId": "ufoco.tasarinan.github.io",
    "files": [
      "dist",
      "main.js",
      "package.json"
    ],
    "publish": {},
    "dmg": {
      "contents": [
        {
          "x": 130,
          "y": 220
        },
        {
          "x": 410,
          "y": 220,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "win": {
      "target": [
        "nsis",
        "msi"
      ]
    },
    "linux": {
      "target": [
        "deb",
        "rpm",
        "snap",
        "AppImage"
      ],
      "category": "Development"
    },
    "directories": {
      "buildResources": "resources",
      "output": "release"
    }
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "setupFiles": [
      "raf/polyfill",
      "<rootDir>/@test/config/setupTests.js"
    ],
    "moduleNameMapper": {
      "^.+\\.(css|less|scss)$": "babel-jest",
      "^.+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/@test/config/fileMock.js",
      "^components$": "<rootDir>/@test/config/componentsMock.js"
    },
    "moduleDirectories": [
      "src",
      "@test/src",
      "node_modules"
    ]
  }
}
