{
  "plugins": ["react"],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 11,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "es2020": true,
    "browser": true,
    "node": true
  },
  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "rules": {
    "no-console": "off",
    "no-undef": "warn",
    "no-undefined": "warn",
    "no-unused-vars": "warn",

    "no-constant-condition": "warn",
    "react/prop-types": "warn"
  },
  "settings": {
    "react": {
      "version": "16"
    }
  }
}
